{
  "name": "spooner-web/just_news",
  "type": "typo3-cms-extension",
  "description": "Easy, flexible and lightweight news extension. No extra tables needed, using pages for news articles.",
  "homepage": "https://git.spooner.io/spooner/just_news",
  "license": "GPL-2.0+",
  "support": {
    "issues": "https://git.spooner.io/spooner/just_news/issues",
    "source": "https://git.spooner.io/spooner/just_news"
  },
  "keywords": [
    "TYPO3",
    "extension",
    "news",
    "easy",
    "lightweight"
  ],
  "authors": [
    {
      "name": "Thomas LÃ¶ffler",
      "email": "loeffler@spooner-web.de",
      "homepage": "https://spooner-web.de",
      "role": "Developer"
    }
  ],
  "require": {
    "typo3/cms-core": "^9.5 || ^10.4 || ^11.4",
    "typo3/cms-fluid-styled-content": "^9.5 || ^10.4 || ^11.4"
  },
  "require-dev": {
    "typo3/cms-filelist": "^9.5 || ^10.4 || ^11.4",
    "typo3/cms-install": "^9.5 || ^10.4 || ^11.4",
    "typo3/cms-tstemplate": "^9.5 || ^10.4 || ^11.4",
    "typo3/testing-framework": "^6.11",
    "helhum/typo3-console": "^5.0 || ^6.0 || ^7.0"
  },
  "config": {
    "vendor-dir": ".Build/vendor/",
    "bin-dir": ".Build/bin/"
  },
  "extra": {
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": ".Build/Web",
      "extension-key": "just_news"
    }
  }
}
